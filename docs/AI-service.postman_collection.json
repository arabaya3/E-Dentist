          "raw": "{\n  \"type\": \"turn\",\n  \"sessionId\": \"demo-session-123\",\n  \"role\": \"user\",\n  \"text\": \"I need to book a cleaning tomorrow afternoon\",\n  \"timestamp\": {{now}}\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/analytics/events",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "analytics",
            "events"
          ]
