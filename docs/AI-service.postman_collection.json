          "raw": "{\n  \"patient\": {\n    \"firstName\": \"Ahmad\",\n    \"lastName\": \"Hassan\",\n    \"phone\": \"+962700000000\"\n  },\n  \"service\": \"cleaning\",\n  \"startTime\": \"2025-11-15T14:30:00Z\",\n  \"notes\": \"prefers Arabic\",\n  \"metadata\": {\n    \"sessionId\": \"demo-session-123\"\n  }\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/integrations/pms/{{provider}}/book",
